    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ projet.id }}</td>
            </tr>
            <tr>
                <th>Intitule</th>
                <td>{{ projet.intitule }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
